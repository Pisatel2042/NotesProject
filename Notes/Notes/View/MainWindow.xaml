<Window x:Class="Notes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notes"
        mc:Ignorable="d"
        Title="Notes" Height="450" Width="800">
    <Grid Margin="0,0,0,-16" >
        <DataGrid ItemsSource="{Binding Task_Note}" AutoGenerateColumns="True" Margin="0,10,0,369"/>
        <Label x:Name="DateLabel" Content="Date" HorizontalAlignment="Left" Margin="55,248,0,0" VerticalAlignment="Top" FontSize="18"/>
        <TextBox HorizontalAlignment="Left" Height="17" Margin="230,316,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="125" Background="#FFEA0808" RenderTransformOrigin="0.499,0.538" BorderBrush="#FF2D5EEF"/>
    </Grid>
</Window>
